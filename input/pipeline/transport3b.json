{
    "sharding": [
        "Scenario"
    ],
    "flows": [
        [
            {
                "$": "load",
                "from": "_Production",
                "sharding": [
                    "Scenario"
                ]
            },
            {
                "$": "filter",
                "column": "TECHNOLOGY:Service",
                "in": [
                    "FRT"
                ]
            },
            {
                "$": "filter",
                "column": "TECHNOLOGY:SubSector",
                "in": [
                    "FLT"
                ]
            },
            {
                "$": "aggregate",
                "groupby": [
                    "YEAR",
                    "TECHNOLOGY:Type"
                ]
            },
            {
                "$": "save",
                "to": "Transport3B",
                "sharding": [
                    "Scenario"
                ],
                "stats": {
                    "xaxis": "YEAR"
                }
            }
        ]
    ]
}