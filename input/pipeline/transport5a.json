{
    "sharding": [
        "Scenario"
    ],
    "flows": [
        [
            {
                "$": "load",
                "from": "_TechnologyEmission",
                "sharding": [
                    "Scenario"
                ]
            },
            {
                "$": "filter",
                "column": "EMISSION",
                "in": [
                    "NOx"
                ]
            },
            {
                "$": "aggregate",
                "groupby": [
                    "YEAR",
                    "TECHNOLOGY:TraMode"
                ]
            },
            {
                "$": "save",
                "to": "Transport5A",
                "sharding": [
                    "Scenario"
                ],
                "stats": {
                    "xaxis": "YEAR"
                }
            }
        ]
    ]
}